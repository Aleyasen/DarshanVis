{
    "id": 12,
    "name": "io-type",
    "type": "bar.scatter",
    "highchart-confs": {
        "chart": {
            "zoomType": "xy"
        },
        "title": {
            "text": "Breakdown of total run time for each job"
        },
        "subtitle": {
            "text": ""
        },
        "xAxis": [
            {
                "title": {
                    "text": "Jobs"
                },
                "crosshair": true
            }
        ],
        "yAxis": [
            {
                "min": 0,
                "labels": {
                    "format": "{value}"
                },
                "title": {
                    "text": "Total Time (%)"
                }
            },
            {
                "min": 1,
                "type": "logarithmic",
                "gridLineWidth": 0,
                "title": {
                    "text": "Number of Processes",
                    "style": {
                        "color": "Highcharts.getOptions().colors[0]"
                    }
                },
                "labels": {
                    "style": {
                        "color": "Highcharts.getOptions().colors[0]"
                    }
                }
                ,
                "opposite": true
            },
            {
                "min": 1,
                "type": "logarithmic",
                "gridLineWidth": 0,
                "title": {
                    "text": "Total Bytes Read/Written",
                    "style": {
                        "color": "Highcharts.getOptions().colors[1]"
                    }
                },
                "labels": {
                    "style": {
                        "color": "Highcharts.getOptions().colors[1]"
                    }
                },
                "opposite": true
            }
        ],
        "tooltip": {
            "shared": true
        }
    },
    "series": [
        {
            "type": "stacked",
            "name": "series1",
            "query": {
                "select": "id, nprocs, total_bytes, (unique_iotime - unique_meta) as localio,unique_meta as localmeta, (shared_time_by_cumul_io_only - shared_time_by_cumul_meta_only) as globalio, (iotime - unique_iotime) as globalmeta, (runtime - iotime) as notio",
                "from": "jobs_all"
            },
            "title1": "Non-global Data I/O",
            "attr1": "localio",
            "title2": "Non-global Metadata",
            "attr2": "localmeta",
            "title3": "Global Data I/O",
            "attr3": "globalio",
            "title4": "Global Metadata",
            "attr4": "globalmeta",
            "title5": "Not I/O",
            "attr5": "notio",
            "title6": "Number of Processes",
            "attr6": "nprocs",
            "title7": "Total Bytes Read/Written",
            "attr7": "total_bytes"
        }
    ]
}